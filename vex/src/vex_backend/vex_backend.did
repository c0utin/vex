type Result = variant { Ok : User; Err : text };
type Result_1 = variant { Ok; Err : text };
type User = record {
  id : nat64;
  name : text;
  created_at : nat64;
  email : text;
};
service : {
  create_user : (text, text) -> (Result);
  delete_user : (nat64) -> (Result_1);
  get_all_users : () -> (vec User) query;
  get_user : (nat64) -> (opt User) query;
  heart_beat : (text) -> (text) query;
  update_user : (nat64, opt text, opt text) -> (Result);
}
